<script src="//cdn.staticfile.org/axios/0.15.3/axios.min.js"></script>
<script src="//cdn.staticfile.org/vue/2.1.6/vue.min.js"></script>
<script src="//cdn.staticfile.org/element-ui/1.0.8/index.js"></script>
<script src="//cdn.staticfile.org/validator/6.2.0/validator.min.js"></script>
<script>
axios.defaults.validateStatus = function () {
    return true
}
axios.interceptors.response.use(function (config) {
    if (config.status >= 400 && config.status !== 404) {
        return Promise.reject(config.data)
    } else if (config.status === 404) {
        message.warn('获取数据失败，请刷新重试！', 5000)
    } else {
        return config
    }
    return config
}, function (e) {
    return Promise.reject(e)
})
</script>